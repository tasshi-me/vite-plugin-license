{
  "name": "vite-plugin-license",
  "version": "0.0.2",
  "description": "Vite plugin to add license banner to bundled output. Tiny wrapper for rollup-plugin-license.",
  "keywords": [
    "vite-plugin",
    "vite",
    "typescript",
    "license"
  ],
  "homepage": "https://github.com/tasshi-me/vite-plugin-license",
  "bugs": {
    "url": "https://github.com/tasshi-me/vite-plugin-license/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tasshi-me/vite-plugin-license.git"
  },
  "license": "MIT",
  "author": {
    "name": "tasshi (Masaharu Tashiro)",
    "email": "tasshi.me@gmail.com",
    "url": "https://tasshi.me/"
  },
  "type": "module",
  "exports": "./lib/index.js",
  "main": "lib/index.js",
  "files": [
    "CHANGELOG.md",
    "lib"
  ],
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "fix": "pnpm run \"/^fix:.*/\"",
    "fix:eslint": "pnpm lint:eslint --fix",
    "fix:prettier": "pnpm lint:prettier --write",
    "lint": "pnpm run \"/^lint:.*/\"",
    "lint:eslint": "eslint . --max-warnings 0",
    "lint:package": "publint",
    "lint:prettier": "prettier --check '**/*.{json,json5,md,yml,yaml}' --cache",
    "test": "vitest"
  },
  "dependencies": {
    "rollup-plugin-license": "^3.6.0"
  },
  "devDependencies": {
    "@cybozu/eslint-config": "^24.3.0",
    "@types/node": "^22.19.1",
    "eslint": "^9.39.1",
    "eslint-plugin-package-json": "^0.77.0",
    "prettier": "^3.6.2",
    "publint": "^0.3.15",
    "rollup": "^4.53.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.9"
  },
  "peerDependencies": {
    "vite": "^7.2.2"
  },
  "packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c"
