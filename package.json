{
  "name": "vite-plugin-license",
  "version": "0.0.2",
  "description": "Vite plugin to add license banner to bundled output. Tiny wrapper for rollup-plugin-license.",
  "keywords": [
    "vite-plugin",
    "vite",
    "typescript",
    "license"
  ],
  "homepage": "https://github.com/tasshi-me/vite-plugin-license",
  "bugs": {
    "url": "https://github.com/tasshi-me/vite-plugin-license/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tasshi-me/vite-plugin-license.git"
  },
  "license": "MIT",
  "author": {
    "name": "tasshi (Masaharu Tashiro)",
    "email": "tasshi.me@gmail.com",
    "url": "https://tasshi.me/"
  },
  "type": "module",
  "exports": "./lib/index.js",
  "main": "lib/index.js",
  "files": [
    "CHANGELOG.md",
    "lib"
  ],
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "fix": "pnpm run \"/^fix:.*/\"",
    "fix:eslint": "pnpm lint:eslint --fix",
    "fix:prettier": "pnpm lint:prettier --write",
    "lint": "pnpm run \"/^lint:.*/\"",
    "lint:eslint": "eslint . --max-warnings 0",
    "lint:package": "publint",
    "lint:prettier": "prettier --check '**/*.{json,json5,md,yml,yaml}' --cache",
    "test": "vitest"
  },
  "dependencies": {
    "rollup-plugin-license": "^3.6.0"
  },
  "devDependencies": {
    "@cybozu/eslint-config": "^24.3.0",
    "@types/node": "^22.18.13",
    "eslint": "^9.38.0",
    "eslint-plugin-package-json": "^0.59.0",
    "prettier": "^3.6.2",
    "publint": "^0.3.15",
    "rollup": "^4.52.5",
    "typescript": "^5.9.3",
    "vitest": "^4.0.4"
  },
  "peerDependencies": {
    "vite": "^7.1.12"
  },
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8"
}
