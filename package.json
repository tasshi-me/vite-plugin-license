{
  "name": "vite-plugin-license",
  "version": "0.0.1",
  "description": "Vite plugin to add license banner to bundled output. Tiny wrapper for rollup-plugin-license.",
  "keywords": [
    "vite-plugin",
    "vite",
    "typescript",
    "license"
  ],
  "homepage": "https://github.com/tasshi-me/vite-plugin-license",
  "bugs": {
    "url": "https://github.com/tasshi-me/vite-plugin-license/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tasshi-me/vite-plugin-license.git"
  },
  "license": "MIT",
  "author": "tasshi (Masaharu Tashiro) <tasshi.me@gmail.com>",
  "type": "module",
  "exports": "./lib/index.js",
  "main": "lib/index.js",
  "files": [
    "CHANGELOG.md",
    "lib"
  ],
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "fix": "pnpm run \"/^fix:.*/\"",
    "fix:eslint": "pnpm lint:eslint --fix",
    "fix:prettier": "pnpm lint:prettier --write",
    "lint": "pnpm run \"/^lint:.*/\"",
    "lint:eslint": "eslint . --max-warnings 0",
    "lint:package": "publint",
    "lint:prettier": "prettier --check '**/*.{json,json5,md,yml,yaml}' --cache",
    "test": "vitest"
  },
  "dependencies": {
    "rollup-plugin-license": "^3.6.0"
  },
  "devDependencies": {
    "@cybozu/eslint-config": "^24.0.0",
    "@types/node": "^22.15.32",
    "eslint": "^9.29.0",
    "eslint-plugin-package-json": "^0.37.0",
    "prettier": "^3.5.3",
    "publint": "^0.3.12",
    "rollup": "^4.44.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "vite": "^6.3.5"
  },
  "packageManager": "pnpm@10.12.2+sha512.a32540185b964ee30bb4e979e405adc6af59226b438ee4cc19f9e8773667a66d302f5bfee60a39d3cac69e35e4b96e708a71dd002b7e9359c4112a1722ac323f"
